#!/bin/sh
  FN=$(echo "${1}" | tr '[:upper:]' '[:lower:]')
  shift

  case "${FN}" in
    "nametoversion")
      NAME=$(echo "${1}" | tr '[:upper:]' '[:lower:]')
      case ${NAME} in
        "duke") echo "15" ;;
        "forky") echo "14" ;;
        "trixie") echo "13" ;;
        "bookworm") echo "12" ;;
        "bullseye") echo "11" ;;
      esac
    ;;


    "versiontoname")
      VERSION=$(echo "${1}" | awk -F '.' '{print $1}')
      case ${VERSION} in
        "15") echo "duke" ;;
        "14") echo "forky" ;;
        "13") echo "trixie" ;;
        "12") echo "bookworm" ;;
        "11") echo "bullseye" ;;
      esac
    ;;

    *) 
      eleven log warning "function ${FN} does not exist"
      exit 1
    ;;
  esac