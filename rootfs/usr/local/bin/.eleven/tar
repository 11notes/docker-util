#!/bin/sh
  FN=$(echo "${1}" | tr '[:upper:]' '[:lower:]')
  shift

  case "${FN}" in
    "?")
      case $(echo "${1}" | awk -F '.' '{print $NF}') in
        "gz") echo "xz";;
        "xz") echo "xJ";;
      esac
    ;;

    *) 
      eleven log warning "function ${FN} does not exist"
      exit 1
    ;;
  esac