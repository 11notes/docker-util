#!/bin/sh
  FN=$(echo "${1}" | tr '[:upper:]' '[:lower:]')
  shift

  case "${FN}" in
    *) 
      eleven checkStatic ${FN}
      eleven strip ${FN}
      mkdir -p /distroless/usr/local/bin
      cp ${FN} /distroless/usr/local/bin
    ;;

    "nostrip") 
      eleven checkStatic ${1}
      mkdir -p /distroless/usr/local/bin
      cp ${1} /distroless/usr/local/bin
    ;;
  esac